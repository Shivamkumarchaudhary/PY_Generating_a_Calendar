   "source": [
    "import calendar\n",
    "\n",
    "year = int (input(\"Enter the year : \"))\n",
    "cal = calendar.TextCalendar(calendar.SUNDAY)  # sunday\n",
    "i =1    # month\n",
    "while i<=12:  \n",
    "    cal.prmonth(year,i)\n",
    "    i+=1"
   ]
